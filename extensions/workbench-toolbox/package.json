{
  "name": "@insightpulseai/workbench-toolbox",
  "version": "0.1.0",
  "description": "TypeScript client for GenAI Toolbox - DB tool plane for workbench",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "toolbox",
    "genai",
    "mcp",
    "database",
    "workbench"
  ],
  "author": "InsightPulseAI <dev@insightpulseai.net>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Insightpulseai-net/pulser-agent-framework"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
